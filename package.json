{
  "name": "rollercoin_main_frontend",
  "version": "1.0.0",
  "engines": {
    "node": ">=18.16.1",
    "npm": ">=9.5.1"
  },
  "private": true,
  "scripts": {
    "start": "pm2 start ecosystem.config.js --env production",
    "start:dev": "pm2 start ecosystem.config.js --env development",
    "start:local": "concurrently --kill-others npm:local:*",
    "start:local_dev": "NODE_ENV=development DISABLE_AUTO_FIX=1 concurrently --kill-others npm:local:*",
    "start:local_dev_ssl": "npm run generate:cert && npm run ssl:proxy:local & npm run start:local_dev",
    "build": "concurrently npm:webpack:prod",
    "build:dev": "concurrently npm:webpack:dev",
    "webpack:prod": "webpack --config webpack.prod.js",
    "webpack:dev": "webpack --config webpack.dev.js",
    "local:webpack_dev_server": "DISABLE_MINIFICATION=1 webpack-dev-server --config ./webpack.local.js",
    "local:nodemon": "nodemon --watch config --watch server ./server/index.js",
    "lint": "npm run lint:js",
    "lint:js": "eslint src --fix",
    "prettier": "prettier --config .prettierrc.js \"./**/*.+(js|json|jsx)\" --write",
    "lint:css": "stylelint \"**/*.{scss,sass}\" --fix",
    "prepare": "husky install",
    "generate:cert": "chmod +x ./create_local_cert.sh && ./create_local_cert.sh -d home.rollercoin.com",
    "ssl:proxy:local": "local-ssl-proxy -s 8091 -t 8090 -k ~/.bmlabs-ssl/home.rollercoin.com/server.key -c ~/.bmlabs-ssl/home.rollercoin.com/server.crt -n home.rollercoin.com",
    "kill:process:local": "kill -9 $(lsof -t -i:8090) & kill -9 $(lsof -t -i:8091)"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "npx eslint --fix"
    ],
    "**/*.{scss,sass}": [
      "npx stylelint --fix"
    ]
  },
